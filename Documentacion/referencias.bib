%%==============================================================================%%
%% Ejemplos de cómo llevar las referencias para el TFG%%
%%==============================================================================%%
%%En el caso de utilizar información obtenida de un artículo de revista científica%%
@article{NIDHRA2012BLACKBOX,
	title={{Blackbox and Whitebox Testing Techniques - A Literature Review}},
	author={Nidhra, S. and Dondeti, J.},
	journal={International Journal of Embedded Systems and Applications},
	volume={2},
	number={2},
	pages={29--50},
	month = {June},
	year={2012},
}
%%========================================================%%
%%En el caso de utilizar información obtenida de un artículo de conferencia científica%%
@inproceedings{MAKINO2015,
	author={Makino, Y. and Klyuev, V.},
	booktitle={Proceedings of the IEEE 8th International Conference on Intelligent Data Acquisition and Advanced Computing Systems: Technology and Applications (IDAACS)},
	title={{{Evaluation of Web Vulnerability Scanners}}},
	year={2015},
	volume={1},
	pages={399--402},
	address = {Warsaw, PL},
	month={September},
}
%%========================================================%%
%%En el caso de utilizar información obtenida de un sitio web%%
@misc{SKIPFISH2016,
	title = {{{Google Code - Skipfish}}},
	author={{Google}},
	month = {March},
	year = {2016},
	howpublished = {\url{https://code.google.com/archive/p/skipfish/}},
	URL =  {https://code.google.com/archive/p/skipfish},
}
%%========================================================%%
%%En el caso de utilizar información obtenida de un manual de usuario%
@techreport{ACUNETIX,
	title = {{Web Vulnerability Scanner v10 Product Manual}},
	author = {{Acunetix}},
	institution = {Acunetix},
	type = {{{Product Manual}}},
	month = {June},
	year = {2015},
}
%%========================================================%%
%%En el caso de utilizar información obtenida de un libro%%
@book{DOOLEY2011SOFTWARE,
	title={Software Development and Professional Practice},
	author={Dooley, J.},
	isbn={9781430238027},
	year={2011},
	publisher={Apress}
}
